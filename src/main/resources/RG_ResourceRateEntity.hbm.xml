<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_ResourceRateEntity" table="RG_ResourceRate" schema="testdatabase">
        <id name="id">
            <column name="id" sql-type="varchar(200)" length="100"/>
        </id>

        <one-to-one name="idResource" property-ref="resourceRate" cascade="all"/>

        <many-to-one name="idSnapshot" class="com.rengu.entity.RG_SnapshotNodeEntity">
            <column name="idSnapshot" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>