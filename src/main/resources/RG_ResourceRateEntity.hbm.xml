<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_ResourceRateEntity" table="RG_ResourceRate" schema="testdatabase">
        <id name="id">
            <column name="id" sql-type="varchar(200)" length="100"/>
        </id>

        <property name="idSnapshot">
            <column name="idSnapshot" sql-type="varchar(200)" not-null="false"/>
        </property>

        <property name="idResource">
            <column name="idResource" sql-type="varchar(200)" not-null="false"/>
        </property>

        <property name="rate">
            <column name="rate" sql-type="double" not-null="false"/>
        </property>

    </class>
</hibernate-mapping>