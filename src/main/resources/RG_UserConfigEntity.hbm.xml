<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_UserConfigEntity" table="RG_UserConfig" schema="testdatabase">
        <id name="id">
            <column name="id" sql-type="int"/>
            <generator class="identity"/>
        </id>
        <property name="latestScheduleId">
            <column name="latestScheduleId" sql-type="varchar(100)" length="20" not-null="false"/>
        </property>
        <property name="currScheduleId">
            <column name="currScheduleId" sql-type="varchar(100)" length="20" not-null="false"/>
        </property>
        <property name="rootSnapshotId">
            <column name="rootSnapshotId" sql-type="varchar(100)" length="20" not-null="false"/>
        </property>
        <property name="middleSnapshotId">
            <column name="middleSnapshotId" sql-type="varchar(100)" length="20" not-null="false"/>
        </property>
        <property name="bottomSnapshotId">
            <column name="bottomSnapshotId" sql-type="varchar(100)" length="20" not-null="false"/>
        </property>
        <property name="errorSchedule">
            <column name="errorSchedule" sql-type="tinyint(1)" not-null="false"/>
        </property>
        <property name="apsReplyCount">
            <column name="apsReplyCount" sql-type="int" not-null="false"/>
        </property>
        <property name="resetApsTable">
            <column name="resetApsTable" sql-type="tinyint(1)" not-null="false"/>
        </property>
        <property name="errorType">
            <column name="errorType" sql-type="varchar(100)" length="20" not-null="false"/>
        </property>
        <property name="errorId">
            <column name="errorId" sql-type="varchar(100)" length="20" not-null="false"/>
        </property>
        <property name="dispatchMesSnapshotId">
            <column name="dispatchMesSnapshotId" sql-type="varchar(100)" length="20" not-null="false"/>
        </property>
        <property name="factoryLayoutId">
            <column name="factoryLayoutId" sql-type="varchar(255)" length="20" not-null="false"/>
        </property>

        <many-to-one name="user" class="com.rengu.entity.RG_UserEntity">
            <column name="idUser" not-null="false"/>
        </many-to-one>

    </class>
</hibernate-mapping>