<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_ProviderEntity" table="RG_Provider" schema="testdatabase">
        <id name="id">
            <column name="id" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="color">
            <column name="color" sql-type="varchar(255)" not-null="false"/>
        </property>
        <many-to-one name="clubByIdClub" class="com.rengu.entity.RG_ClubEntity">
            <column name="idClub" not-null="false"/>
        </many-to-one>
        <many-to-one name="siteByIdSite" class="com.rengu.entity.RG_SiteEntity">
            <column name="idSite" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>