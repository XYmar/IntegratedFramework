<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_ProductEntity" table="RG_Product" schema="testdatabase">
        <id name="id">
            <column name="id" sql-type="varchar(100)" length="100"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="mesId">
            <column name="mesId" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="ref">
            <column name="ref" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="depth">
            <column name="depth" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="stock">
            <column name="stock" sql-type="smallint(6)" not-null="false"/>
        </property>
        <property name="unit">
            <column name="unit" sql-type="varchar(20)" length="20" not-null="false"/>
        </property>
        <property name="model">
            <column name="model" sql-type="varchar(100)" length="100" not-null="false"/>
        </property>
        <property name="state">
            <column name="state" sql-type="varchar(20)" length="20" not-null="false"/>
        </property>
        <property name="supplyMethod">
            <column name="supplyMethod" sql-type="varchar(20)" length="20" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>