<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_AdjustOrderEntity" table="RG_AdjustOrder" schema="TestDatabase">
        <id name="id">
            <column name="id" sql-type="varchar(100)" length="100"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="quantity">
            <column name="quantity" sql-type="smallint(8)"/>
        </property>
        <property name="priority">
            <column name="priority" sql-type="smallint(2)"/>
        </property>
        <property name="t0">
            <column name="t0" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="t1">
            <column name="t1" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="t2">
            <column name="t2" sql-type="varchar(20)" length="20"/>
        </property>
        <many-to-one name="productEntity" class="com.rengu.entity.RG_ProductEntity" column="idProduct" unique="true"/>
    </class>
</hibernate-mapping>