<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_RealDataEntity" table="RG_RealData" schema="testdatabase">
        <id name="id">
            <column name="id" sql-type="varchar(255)" length="255"/>
        </id>
        <property name="idResource">
            <column name="idResource" sql-type="varchar(255)"/>
        </property>
        <property name="state">
            <column name="state" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="good">
            <column name="good" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="startLocation">
            <column name="startLocation" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="endLocation">
            <column name="endLocation" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="valueType">
            <column name="valueType" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="value">
            <column name="value" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="idTask">
            <column name="idTask" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>