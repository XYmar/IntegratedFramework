<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.devMode" value="true"></constant>

    <package name="default" extends="struts-default" namespace="/"></package>

    <package name="Login" namespace="/Login" extends="default">
        <action name="doLoginAction" class="com.rengu.actions.doLoginAction">
            <result name="success">/index.jsp</result>
            <result name="error">/jsps/LoginPage/Login.jsp</result>
        </action>
    </package>
</struts>