<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.devMode" value="true"/>
    <constant name="struts.action.excludePattern" value="/notification"></constant>


    <package name="default" extends="struts-default" namespace="/">
        <action name="UsersAction" class="com.rengu.actions.UsersAction" method="doLogin">
            <result name="error">/jsps/Login/Login_Fail.jsp</result>
            <result name="success">/jsps/Login/Login_Success.jsp</result>
        </action>
    </package>
</struts>
