<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_ClubEntity" table="RG_Club" schema="testdatabase">
        <id name="id">
            <column name="id" sql-type="varchar(255)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)" not-null="false"/>
        </property>
        <set name="usersById" inverse="true">
            <key>
                <column name="idClub" not-null="false"/>
            </key>
            <one-to-many not-found="ignore" class="com.rengu.entity.RG_UserEntity"/>
        </set>
    </class>
</hibernate-mapping>