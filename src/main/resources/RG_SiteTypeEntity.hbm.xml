<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rengu.entity.RG_SiteTypeEntity" table="RG_SiteType" schema="testdatabase">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="resource">
            <column name="resource" sql-type="varchar(255)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="x">
            <column name="x" sql-type="int" not-null="false"/>
        </property>
        <property name="y">
            <column name="y" sql-type="int" not-null="false"/>
        </property>
        <property name="color">
            <column name="color" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="idIcon">
            <column name="idIcon" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="sizeIcon">
            <column name="sizeIcon" sql-type="int" not-null="false"/>
        </property>
        <property name="capacity">
            <column name="capacity" sql-type="int" not-null="false"/>
        </property>
        <property name="layoutType">
            <column name="layoutType" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>